          <tgroup cols="4">
            <colspec colname="c1" colnum="1" colwidth="28.15pt"/>
            <colspec colname="c2" colnum="2" colwidth="41.89pt"/>
            <colspec colname="c3" colnum="3" colwidth="87.71pt"/>
            <colspec colname="c4" colnum="4" colwidth="65.45pt"/>
            <thead>
              <row>
                <entry>Supplier</entry>
                <entry>Fruit</entry>
                <entry>Ref</entry>
                <entry>Depot</entry>
              </row>
            </thead>
            <tbody>
              <row>
                <entry>OrangesRus</entry>
                <entry>Orange</entry>
                <entry>12-89763-1</entry>
                <entry>Seville</entry>
              </row>
              <row>
                <entry>PearPackers</entry>
                <entrytbl cols="3" namest="c2" nameend="c4">
                  <tbody>
                    <row>
                        <entry>Australia</entry>
                        <entry>Packham</entry>
                        <entry>Eating</entry>
                    </row>
                    <row>
                        <entry morerows="1">UK</entry>
                        <entry>Conference</entry>
                        <entry>Eating</entry>
                    </row>
                    <row>
                      <entry>Black Worcester</entry>
                      <entry>Cooking</entry>
                    </row>
                    <row>
