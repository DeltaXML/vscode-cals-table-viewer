                    </row>
                    <row>
                      <entry>France</entry>
                      <entry>Claude Blanchet</entry>
                      <entry>Cooking and Eating</entry>
                    </row>
                  </tbody>
                </entrytbl>
              </row>
              <row>
                <entry>KiwisKiwis</entry>
                <entry>Kiwi</entry>
                <entry>77777</entry>
                <entry>Christchurch</entry>
              </row>
              <row>
                <entry>Adams</entry>
                <entry>Apple</entry>
                <entry>99999</entry>
                <entry>Worcester</entry>
              </row>
            </tbody>
