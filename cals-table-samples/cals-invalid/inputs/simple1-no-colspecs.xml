        <section>
            <title>Replace this with processor setup details</title>
            <p>
                <table frame="all" rowsep="1" colsep="1" id="table_lcl_25w_sjb">
                    <title>Example 1: Content only changed</title>
                    <!-- The A table is the basic starter version -->
                    <tgroup cols="3">
                        <tbody>
                            <row><entry>a</entry><entry>b</entry><entry>c</entry></row>
                            <row><entry>a3</entry><entry>b3</entry><entry>c3</entry></row>
                            <row><entry>a2.2</entry><entry>b2.2</entry><entry>c2.2</entry></row>
                        </tbody>
                    </tgroup>
                </table>
            </p>
        </section>
