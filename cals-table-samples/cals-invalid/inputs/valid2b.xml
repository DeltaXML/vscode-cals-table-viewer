<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_th2_c5w_sjb">
  <title>Examples</title>
  <!-- See our web page https://docs.deltaxml.com/support/latest/documentation/comparing-document-tables-cals-or-html for illustrations -->
  <body>
    <section>
      <title>Replace this with processor setup details</title>
      <p>
        <table frame="all" rowsep="1" colsep="1" id="table_lcl_25w_sjb">
          <title>Example 1: Content only changed</title>
          <!-- The A table is the basic starter version -->
          <tgroup cols="3">
            <colspec colname="a"/>
            <colspec colname="b"/>
            <colspec colname="c"/>
            <spanspec spanname="ab" namest="a" nameend="b"/>
            <tbody>
              <row><entry>a</entry><entry>b</entry><entry>c</entry></row>
              <row><entry spanname="ab">ab</entry><entry>c</entry></row>
              <row><entry namest="a">a</entry><entry namest="b" nameend="c">bc</entry></row>
              <row><entry colname="a">a</entry><entry namest="b" nameend="c" morerows="1">bc</entry></row>
              <row><entry colname="a">a</entry></row>
            </tbody>
          </tgroup>
        </table>
      </p>
    </section>
  </body>
</topic>