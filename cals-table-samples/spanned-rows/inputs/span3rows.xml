<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_th2_c5w_sjb">
  <title>Examples</title>
  <!-- See our web page https://docs.deltaxml.com/support/latest/documentation/comparing-document-tables-cals-or-html for illustrations -->
  <body>
    <section>
      <title>Replace this with processor setup details</title>
      <p>
        <table frame="all" rowsep="1" colsep="1" id="table_lcl_25w_sjb">
          <title>No rowspans or colspans</title>
          <!-- The A table is the basic starter version -->
          <tgroup cols="4">
            <colspec colname="c1" colnum="1" colwidth="28.15pt"/>
            <colspec colname="c2" colnum="2" colwidth="41.89pt"/>
            <colspec colname="c3" colnum="3" colwidth="87.71pt"/>
            <colspec colname="c4" colnum="4" colwidth="65.45pt"/>
            <thead>
              <row>
                <entry>Sr</entry>
                <entry>Name</entry>
                <entry>Email</entry>
                <entry>DOB</entry>
              </row>
            </thead>
            <tbody>
              <row>
                <entry>1</entry>
                <entry>Joe</entry>
                <entry>joe@gmail.com</entry>
                <entry>1-1-1985</entry>
              </row>
              <row>
                <entry>2</entry>
                <entry morerows="2">Anna</entry>
                <entry>anna@gmail.com</entry>
                <entry morerows="2">2-2-1985</entry>
              </row>
              <row>
                <entry>2a</entry>
                <entry>anna@hotmail.co.uk</entry>
              </row>
              <row>
                <entry>2b</entry>
                <entry>anna@yahoo.com</entry>
              </row>
              <row>
                <entry>4</entry>
                <entry>Dave</entry>
                <entry>dave@gmail.com</entry>
                <entry>4-4-1985</entry>
              </row>
            </tbody>
          </tgroup>
        </table>
      </p>
    </section>
  </body>
</topic>