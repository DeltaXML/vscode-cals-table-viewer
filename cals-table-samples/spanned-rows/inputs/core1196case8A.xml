<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_th2_c5w_sjb">
  <title>Examples</title>
  <!-- See our web page https://docs.deltaxml.com/support/latest/documentation/comparing-document-tables-cals-or-html for illustrations -->
  <body>
    <section>
      <title>Replace this with processor setup details</title>
      <p>
        <table>
          <title>Sample Table</title>
          <tgroup cols="3">
            <colspec colnum="1" colname="col1" colwidth="1.0*"/>
            <colspec colnum="2" colname="col2" colwidth="1.0*"/>
            <colspec colnum="3" colname="col3" colwidth="1.0*"/>
            <thead>
              <row>
                <entry>
                  <para>Col 1</para>
                </entry>
                <entry>
                  <para>Col 2</para>
                </entry>
                <entry>
                  <para>Col 3</para>
                </entry>
              </row>
            </thead>
            <tbody>
              <row>
                <entry>
                  <para>a1</para>
                </entry>
                <entry>
                  <para>b1</para>
                </entry>
                <entry morerows="2">
                  <para>c1</para>
                </entry>
              </row>
              <row>
                <entry>
                  <para>a2</para>
                </entry>
                <entry>
                  <para>b2</para>
                </entry>
              </row>
              <row>
                <entry>
                  <para>a3</para>
                </entry>
                <entry>
                  <para>b3</para>
                </entry>
              </row>
            </tbody>
          </tgroup>
        </table>
      </p>
    </section>
  </body>
</topic>