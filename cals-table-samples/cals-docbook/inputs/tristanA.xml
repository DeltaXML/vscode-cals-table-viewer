<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_th2_c5w_sjb">
  <title>Examples</title>
  <!-- See our web page https://docs.deltaxml.com/support/latest/documentation/comparing-document-tables-cals-or-html for illustrations -->
  <body>
    <section>
      <title>Replace this with processor setup details</title>
      <p>
        <table frame="all">
          <title>CALS table without colnames</title>
          <tgroup cols="5">
            <colspec colname="c1" colnum="1" colwidth="1*"/>
            <colspec colname="c2" colnum="2" colwidth="1*"/>
            <colspec colname="c3" colnum="3" colwidth="1*"/>
            <colspec colname="c4" colnum="4" colwidth="1*"/>
            <colspec colname="c5" colnum="5" colwidth="1*"/>
            <thead>
              <row>
                <entry>Name</entry>
                <entry>Column name</entry>
                <entry>Rubbish Data</entry>
                <entry>More Rubbish Data</entry>
                <entry>Keep This</entry>
              </row>
            </thead>
            <tbody>
              <row>
                <entry>Tom</entry>
                <entry>Unique Data</entry>
                <entry>0000</entry>
                <entry>XXX</entry>
                <entry>Critical</entry>
              </row>
              <row>
                <entry>Fred</entry>
                <entry morerows="2">Shared Data</entry>
                <entry>12345</entry>
                <entry>ABCDE</entry>
                <entry>Important</entry>
              </row>
              <row>
                <entry>Joe</entry>
                <entry>6789</entry>
                <entry>FGHJ</entry>
                <entry>Secure</entry>
              </row>
              <row>
                <entry>John</entry>
                <entry>1010</entry>
                <entry>KLMN</entry>
                <entry>Vital</entry>
              </row>
            </tbody>
          </tgroup>
        </table>
      </p>
    </section>
  </body>
</topic>