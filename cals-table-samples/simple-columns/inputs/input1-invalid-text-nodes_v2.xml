    <section>
      <title>Replace this with processor setup details</title>
      <p>
        <table frame="all" rowsep="1" colsep="1" id="table_lcl_25w_sjb">
          Invalid table text v1
          <title>Example 1: Content only changed</title>
          <!-- The A table is the basic starter version -->
          <tgroup cols="4">
            <colspec colname="c1" colnum="1" colwidth="28.15pt"/>
            <colspec colname="c2" colnum="2" colwidth="41.89pt"/>
            <colspec colname="c3" colnum="3" colwidth="87.71pt"/>
            <colspec colname="c4" colnum="4" colwidth="65.45pt"/>
            Invalid body text v1
            <thead>invalid-row B
              <row>invalid-entry B
                <entry>Sr</entry>
                <entry>Name</entry>
                <entry>Email</entry>
                <entry>DOB</entry>
              </row>
            </thead>
            Invalid body text v2
            <tbody>invalid-row B
              <row>invalid-entry B
                <entry>1</entry>
                <entry>Joe</entry>
                <entry>joe@gmail.com</entry>
                <entry>1-1-1985</entry>
              </row>
              <row>
                <entry>2</entry>
                <entry>Anna</entry>
                <entry>anna@gmail.com</entry>
                <entry>2-2-1985</entry>
              </row>
              <row>
                <entry>3</entry>
                <entry>Chris</entry>
                <entry>chris@gmail.com</entry>
                <entry>3-3-1985</entry>invalid-entry B
              </row>
              <row>
                <entry>4</entry>
                <entry>Dave</entry>
                <entry>dave@gmail.com</entry>
                <entry>4-4-1985</entry>
              </row>invalid-row B
            </tbody>
            Invalid body text v2
          </tgroup>
          Invalid table text v2
        </table>
      </p>
    </section>
